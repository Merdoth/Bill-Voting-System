  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "start": "NODE_ENV=production node dist/server/app.js",
    "start:dev": "NODE_ENV=development nodemon --watch server --exec babel-node -- ./server/app.js",
    "build:server": "rm -rf dist && babel ./server/ -d ./dist/server",
    "build": "npm run build:copy && npm run build:webpack",
    "build:webpack": "NODE_ENV=production webpack --config ./webpack.config.prod.babel.js",
    "build:copy": "NODE_ENV=production babel ./ -d dist --copy-files  --ignore node_modules/,dist",
    "heroku-postbuild": "npm run build",
    "test:client": "jest --watch",
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text ./node_modules/.bin/mocha --compilers js:babel-core/register ./server/tests/index.spec.js -t 50000 --exit && npm run coverage"
